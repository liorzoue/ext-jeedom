{
	"name" :"Jeedom",
	"version" :"1.0.0",
	"manifest_version": 2,
	"description" :"Accédez à Jeedom facilement, via votre navigateur !",
	"browser_action" :
	{
		"default_popup" :"layouts/popup.html"
	},
	"permissions": [
		"storage",
		"background",
		"notifications",
		"<all_urls>"
	],
	"background": {
		"page": "layouts/background.html"
	},
	"icons": { 
		"512": "img/house512.png"
	},

	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}